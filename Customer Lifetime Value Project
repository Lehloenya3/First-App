      "source": [
        "import pandas as pd\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "from datetime import datetime, timedelta\n",
        "# I simulated a customers Table\n",
        "customers_data = {\n",
        "    'CustomerID': range(1001, 1021),\n",
        "    'FirstName': ['John', 'Maria', 'Sipho', 'Nandi', 'David', 'Tanya', 'Lerato', 'Michael', 'Mpho', 'Julia',\n",
        "                  'James', 'Dineo', 'Peter', 'Zanele', 'Linda', 'Thabo', 'Emily', 'Kagiso', 'Sarah', 'Jabu'],\n",
        "    'LastName': ['Smith', 'Patel', 'Dlamini', 'Mokoena', 'Naidoo', 'Jones', 'Mofokeng', 'Brown', 'Nkosi', 'Taylor',\n",
        "                 'Murray', 'Mahlangu', 'Parker', 'Khumalo', 'Wilson', 'Modise', 'Stewart', 'Molefe', 'Adams', 'Ndlovu'],\n",
        "    'Age': np.random.randint(18, 65, size=20),\n",
        "    'Gender': np.random.choice(['Male', 'Female'], size=20),\n",
        "    'Location': np.random.choice(['Johannesburg', 'Cape Town', 'Durban', 'Pretoria', 'Port Elizabeth'], size=20),\n",
        "    'IncomeLevel': np.random.choice(['Low', 'Medium', 'High'], size=20)\n",
        "}\n",
        "\n",
        "customers_df = pd.DataFrame(customers_data)\n",
        "\n",
        "# Simulate Transactions Table\n",
        "transaction_dates = [datetime(2024, 1, 1) + timedelta(days=np.random.randint(0, 60)) for _ in range(40)]\n",
        "transactions_data = {\n",
        "    'TransactionID': range(2001, 2041),\n",
        "    'CustomerID': np.random.choice(customers_data['CustomerID'], size=40),\n",
        "    'Date': transaction_dates,\n",
        "    'Amount': np.random.uniform(100, 5000, size=40),\n",
        "    'ProductType': np.random.choice(['Savings Account', 'Credit Card', 'Fixed Deposit'], size=40),\n",
        "    'Branch': np.random.choice(['Johannesburg', 'Cape Town', 'Durban', 'Pretoria', 'Port Elizabeth'], size=40)\n",
        "}\n",
        "\n",
        "transactions_df = pd.DataFrame(transactions_data)\n",
        "\n",
        "# Simulate ProductUsage Table\n",
        "usage_data = {\n",
        "    'UsageID': range(3001, 3021),\n",
        "    'CustomerID': np.random.choice(customers_data['CustomerID'], size=20),\n",
        "    'ProductType': np.random.choice(['Savings Account', 'Credit Card', 'Fixed Deposit'], size=20),\n",
        "    'UsageFrequency': np.random.choice(['Monthly', 'Weekly', 'Quarterly'], size=20),\n",
        "    'LastUsed': [datetime(2024, 2, 1) + timedelta(days=np.random.randint(0, 30)) for _ in range(20)]\n",
        "}\n",
        "\n",
        "product_usage_df = pd.DataFrame(usage_data)\n",
        "\n",
        "# Simulate CustomerInteractions Table\n",
        "interaction_data = {\n",
        "    'InteractionID': range(4001, 4021),\n",
        "    'CustomerID': np.random.choice(customers_data['CustomerID'], size=20),\n",
        "    'InteractionType': np.random.choice(['Service Inquiry', 'Product Inquiry', 'Complaint'], size=20),\n",
        "    'Date': [datetime(2024, 1, 1) + timedelta(days=np.random.randint(0, 60)) for _ in range(20)],\n",
        "    'Notes': ['Inquired about loan options', 'Asked about credit card benefits', 'Reported issue with online banking',\n",
        "              'Requested account statement', 'Inquired about investment options', 'Reported lost card',\n",
        "              'Asked about online banking', 'Inquired about mortgage rates', 'Reported fraudulent transaction',\n",
        "              'Inquired about account balance', 'Requested change of address', 'Asked about insurance products',\n",
        "              'Inquired about branch locations', 'Requested account closure', 'Reported unauthorized transaction',\n",
        "              'Asked about interest rates', 'Inquired about foreign exchange rates', 'Reported service dissatisfaction',\n",
        "              'Asked about account fees', 'Inquired about business banking services']\n",
        "}\n",
        "\n",
        "customer_interactions_df = pd.DataFrame(interaction_data)\n",
        "\n",
        "customers_df.to_csv('customers_data.csv', index=False)  # Save customers_df\n",
        "transactions_df.to_csv('transactions.csv', index=False)  # Save transactions_df\n",
        "product_usage_df.to_csv('product_usage.csv', index=False)  # Save product_usage_df\n",
        "customer_interactions_df.to_csv('customer_interactions.csv', index=False)  # Save customer_interactions_df\n",
        "\n",
        "# Output the DataFrames\n",
        "print(customers_df)\n",
        "print(transactions_df)"
      ]
    },
